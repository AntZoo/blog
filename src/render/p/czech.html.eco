---
title: "In Czech"
isPaged: false
pagedCollection: 'czech'
pageSize: 10
---

<!-- Page Content -->
<% for document in @getPageCollection('czech').toJSON(): %>
    <article id="post" class="post">
        <h3><a href='<%=document.url%>'><%= document.title %></a></h3>
        <div class="post-date"><%= document.date.toLocaleDateString() %></div>
    </article>
<% end %>